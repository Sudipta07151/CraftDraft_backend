{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "product": {
      "model": "product-list",
      "via": "order"
    },
    "FirstName": {
      "type": "string"
    },
    "LastName": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "city": {
      "type": "enumeration",
      "enum": [
        "KOLKATA"
      ]
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "WESTBENGAL"
      ]
    },
    "orderID": {
      "type": "uid",
      "targetField": "phone"
    },
    "zipcode": {
      "type": "string"
    }
  }
}
